#!/bin/bash
cd ~/work/vendor/realme/RMX1901
aria2c https://github.com/Shravan55555/extras/releases/download/publish/vendor.img
ls
cd ~/work
bash ~/work/vendor/realme/RMX1901/patch/patch.sh
. build/envsetup.sh
export CCACHE_DIR=/tmp/ccache
export CCACHE_EXEC=$(which ccache)
export USE_CCACHE=1
ccache -M 60G
ccache -o compression=true
ccache -z
lunch "$LUNCH_COMBO"
$EXTRA_CMD
mka $TARGET -j8
